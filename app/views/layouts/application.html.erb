<!DOCTYPE html>
<html>
  <head>
    <title>ActorsRolesProducersMarket</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <%= actor_logged_in ? button_to('Logout', logout_path, method: :delete) : link_to('Login', login_path)%>
    <%= production_company_logged_in ? button_to('Logout', logout_path, method: :delete) : link_to('Login', login_path)%>
    <%= yield %>
  </body>
</html>
